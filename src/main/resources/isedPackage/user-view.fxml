         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <CheckBox fx:id="inBox" layoutX="14.0" layoutY="126.0" mnemonicParsing="false" prefHeight="34.0" prefWidth="94.0" text="Вхідні">
         <font>
            <Font size="14.0" />
         </font>
      </CheckBox>
      <CheckBox fx:id="outBox" layoutX="123.0" layoutY="135.0" mnemonicParsing="false" text="Вихідні">
