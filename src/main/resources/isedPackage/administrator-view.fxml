<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="isedPackage.AdministratorController">
   <children>
      <Label layoutY="36.0" prefHeight="36.0" prefWidth="600.0" text="Список користувачів">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <ListView fx:id="listView" layoutX="-1.0" layoutY="78.0" prefHeight="321.0" prefWidth="600.0" />
      <MenuBar layoutX="-1.0" prefHeight="36.0" prefWidth="600.0">
        <menus>
          <Menu mnemonicParsing="false" text="Створити">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#createUser" text="Новий користувач" />
            </items>
          </Menu>
            <Menu mnemonicParsing="false" text="Оновити дані">
              <items>
                <MenuItem mnemonicParsing="false" onAction="#refresh" text="..." />
              </items>
            </Menu>
        </menus>
      </MenuBar>
   </children>
</AnchorPane>
